{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.git.enabled setting": "true",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "publishDate",
          "name": "publishDate",
          "type": "datetime"
        },
        {
          "title": "coverSVG",
          "name": "coverSVG",
          "type": "string"
        },
        {
          "title": "socialImage",
          "name": "socialImage",
          "type": "image"
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/src/content/author",
      "title": "author"
    },
    {
      "path": "[[workspace]]/src/content/category",
      "title": "categorias"
    },
    {
      "path": "[[workspace]]/src/content/doc",
      "title": "docs"
    },
    {
      "title": "contenido",
      "path": "[[workspace]]/src/content"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog",
      "previewPath": "'blog'"
    },
    {
      "title": "Servicios",
      "path": "[[workspace]]/src/content/servicios",
      "previewPath": "'services'"
    }
  ],
  "frontMatter.content.publicFolder": {
    "path": "src/assets",
    "relative": true
  }
}